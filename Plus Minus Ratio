def plusMinus(arr):
    pos_ctr = neg_ctr = zero_ctr = 0
    pos_ratio = neg_ratio = zero_ratio = 0
    n = len(arr)
    for i in arr:
        if i > 0:
            pos_ctr += 1
        elif i < 0:
            neg_ctr += 1
        else:
            zero_ctr += 1
    print(pos_ctr/n)
    print(neg_ctr/n)
    print(zero_ctr/n)
